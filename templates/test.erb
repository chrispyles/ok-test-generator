test = {
	"name": <%= curr_test.test_name %>,
	"suites": [ <% for suite in curr_test.suites %>
		{
			"cases": [ <% for test_case in suite %>
				{
					"code": r"""<% for code_pair in test_case %>
					>>> <%= code_pair.code %>
					<%= code_pair.output %>
					<% end %>
					"""
				}, 
				<% end %>
			]
		}, 
		<% end %>
	]
}