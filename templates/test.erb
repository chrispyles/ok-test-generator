test = {
	"name": "<%= @test_name %>",
	"suites": [ <% for suite in @suites %>
		{
			"cases": [ <% for test_case in suite %>
				{
					"code": r"""<% for code_pair in test_case.cases; if code_pair.code != "" %>
					>>> <%= code_pair.code %>
					<%= code_pair.output %><% end; end %>
					"""
				}, 
				<% end %>
			]
		}, 
		<% end %>
	]
}