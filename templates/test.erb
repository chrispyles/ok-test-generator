test = {
	"name": "<%= @test_name %>",
	"points": <%= @points == "" ? 0 : @points %>,
	"suites": [ <% for suite in @suites %>
		{
			"cases": [ <% for test_case in suite %><% for code in test_case %>
				{
					"code": r"""
					>>> <%= code.code %>
					<%= code.output %>
					""",
					"hidden": <%= code.hidden ? "True" : "False" %>,
					"locked": <%= code.locked ? "True" : "False" %>,
				}, <% end %><% end %>
			],
			"scored": <%= @scored ? "True" : "False" %>
			"setup": "",
			"teardown": "",
			"type": "doctest"
		}, <% end %>
	]
}